{
    "manifest_version": 3,
    "name": "Instagram Customizer",
    "version": "1.0",
    "description": "Customize your Instagram experience by filtering your feed based on interests.",
    "permissions": ["activeTab"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      }
    }
  }
  